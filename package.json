{
  "name": "@madeline/serverless-jest-plugin-v3",
  "version": "0.5.11",
  "description": "Serverless v3+ plugin for test driven development using Jest",
  "main": "./dist/cjs/index.js",
  "author": "keee Engineering Team <engineering@keee.ai>",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "build:esm": "BABEL_ENV=esm babel src --out-dir dist/esm",
    "build:cjs": "BABEL_ENV=cjs babel src --out-dir dist/cjs",
    "build": "npm run build:esm && npm run build:cjs"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keee-ai/serverless-jest-plugin.git"
  },
  "dependencies": {
    "ejs": "^2.6.1",
    "fs-extra": "^3.0.0",
    "jest": "^24.5.0",
    "lambda-wrapper": "^0.3.0",
    "yaml-edit": "^0.1.3"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keee-ai/serverless-jest-plugin/issues"
  },
  "homepage": "https://github.com/keee-ai/serverless-jest-plugin#readme",
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.24.9",
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@babel/preset-env": "^7.24.8",
    "babel-plugin-add-module-exports": "^1.0.4",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "serverless": "^1.32.0",
    "strip-ansi": "^6.0.0"
  }
}
